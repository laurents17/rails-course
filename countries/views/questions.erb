<html>
    <body>
        <a href="/questions/new">New</a>
        <br><br>
        
        <% @questions.each do |q| %>
            <div>
                <a href="/questions/edit?id=<%= q.id %>">Edit</a>
                <a href="/questions/delete?id=<%= q.id %>">Delete</a>
                <%= q.question %>
                <%= q.answer %>
                <img height="100" src="<%= q.flag %>">
            </div>
        <% end %>
        
    </body>
</html>